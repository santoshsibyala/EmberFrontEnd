<h2>Below are the posts</h2>
<div class="row">
<div class="col-md-6">
<ul>



        {{#each content as |mod|}}

        <li>
                Title: {{mod.title}}
               <br/>
               <div>Body :{{mod.body}}</div>
            <a href="#" {{action 'deletePost' mod}}>Delete!</a>

        </li>

        {{/each}}
</ul>

  </div>


</div>

{{outlet}}
